<Window x:Class="Conference_Organization.View.Widows.Captcha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Conference_Organization.View.Widows"
        mc:Ignorable="d"
        Title="Captcha" Height="400" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="20" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="20" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <TextBlock Name="CaptchTbl"/>
        <TextBox Name="CaptchTb" Grid.Row="2" HorizontalAlignment="Center"/>
        <Button Name="EnterBtn" Content="Ввод" Grid.Row="4" HorizontalAlignment="Center"/>
    </Grid>
</Window>
